{
  "name": "@ui-composer/mobile-app",
  "version": "0.0.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "clean": "yarn clean:workspace",
    "lint": "yarn lint:workspace",
    "rn": "$(npx react-native run-ios)",
    "expo": "$(yarn bin expo)",
    "start": "EXPO_USE_BETA_CLI=1 EXPO_BETA=1 concurrently 'yarn workspace ui-composer watch' '$(yarn bin expo) start $*'",
    "android": "yarn start --android",
    "ios": "yarn start --ios",
    "web": "yarn start --web",
    "build.ios": "eas build --platform ios --local",
    "build.anroid": "eas build --platform android --local",
    "bundlesize": "npx react-native-bundle-visualizer"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.10",
    "expo": "46.0.0-beta.1",
    "expo-font": "~10.2.0",
    "expo-keep-awake": "~10.2.0",
    "expo-modules-core": "~0.11.3",
    "expo-splash-screen": "~0.16.1",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.1",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "ui-composer": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@ui-composer/eslint-config": "workspace:*",
    "eas-cli": "^0.55.1",
    "expo-yarn-workspaces": "^2.0.0",
    "react-native-bundle-visualizer": "^3.1.1"
  }
}
