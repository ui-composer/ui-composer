{
  "name": "ui-composer",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "format": "yarn exec workspaces-to-typescript-project-references --includesRoot",
    "lint:workspace": "TIMING=1 eslint $INIT_CWD"
  },
  "engines": {
    "node": "16.18.1"
  },
  "packageManager": "yarn@3.2.1",
  "author": "Katherine Martinez <kmartinezmedia@gmail.com>",
  "homepage": "https://github.com/ui-composer/ui-composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-composer/ui-composer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-composer/ui-composer"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  },
  "dependencies": {
    "@manypkg/cli": "^0.19.2",
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.9.0",
    "@react-native-community/eslint-config": "^3.0.3",
    "@types/eslint": "^8.4.10",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-native": "0.70.8",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "nodemon": "^2.0.20",
    "nx": "15.3.3",
    "packemon": "^2.4.1",
    "prettier": "^2.8.1",
    "type-fest": "^2.19.0",
    "typescript": "^4.9.4"
  },
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": false
    }
  }
}
