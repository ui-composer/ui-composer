{
  "name": "@ui-composer/monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "./scripts/clean/workspaces.sh",
    "clean:workspace": "./scripts/clean/workspace.mjs",
    "format": "./scripts/format/workspaces.sh",
    "lint:workspace": "TIMING=1 eslint $INIT_CWD"
  },
  "resolutions": {
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.1",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-native": "0.69.1"
  },
  "engines": {
    "node": "18.12.*"
  },
  "packageManager": "yarn@3.2.1",
  "author": "Katherine Martinez <kmartinezmedia@gmail.com>",
  "homepage": "https://github.com/ui-composer/ui-composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-composer/ui-composer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-composer/ui-composer"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  },
  "devDependencies": {
    "@manypkg/cli": "^0.19.1",
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.8.2",
    "@nrwl/expo": "^15.0.10",
    "@react-native-community/eslint-config": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-mdx": "^2.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nx": "15.0.10",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "zx": "^7.0.7"
  },
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": false
    }
