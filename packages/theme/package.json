{
  "name": "@ui-composer/theme",
  "version": "0.0.0",
  "sideEffects": [
    "*.css"
  ],
  "packemon": [
    {
      "bundle": false,
      "platform": [
        "native",
        "browser"
      ]
    }
  ],
  "module": "./esm/index.js",
  "browser": "./lib/browser/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/browser/index.js"
      },
      "react-native": "./lib/native/index.js",
      "default": "./lib/browser/index.js"
    }
  },
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@ui-composer/palette": "workspace:^",
    "csstype": "^3.1.1",
    "type-fest": "^2.19.0"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@ui-composer/eslint-config": "workspace:*",
    "react": "18.2.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0"
  },
  "author": "Katherine Martinez <kmartinezmedia@gmail.com>",
  "homepage": "https://github.com/ui-composer/ui-composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-composer/ui-composer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-composer/ui-composer",
    "directory": "packages/theme"
  },
  "types": "./dts/index.d.ts",
  "main": "./lib/native/index.js"
}
