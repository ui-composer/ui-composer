{
  "root": "packages/ui-composer",
  "sourceRoot": "packages/ui-composer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "cwd": "packages/ui-composer",
        "commands": ["ui-composer-cli-esbuild"]
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/ui-composer",
        "commands": ["yarn version $npm_package_version --no-git-tag-version"]
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/ui-composer",
        "commands": ["ui-composer-cli-esbuild --watch"]
      }
    }
  }
}
