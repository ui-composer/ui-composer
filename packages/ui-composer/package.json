{
  "name": "ui-composer",
  "version": "0.0.0",
  "sideEffects": [
    "*.css"
  ],
  "packemon": [
    {
      "bundle": false,
      "platform": [
        "native",
        "browser"
      ]
    }
  ],
  "module": "./esm/index.js",
  "browser": "./lib/browser/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/browser/index.js"
      },
      "react-native": "./lib/native/index.js",
      "default": "./lib/browser/index.js"
    }
  },
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "colord": "^2.9.3",
    "csstype": "^3.1.1",
    "lodash": "^4.17.21",
    "type-fest": "^2.16.0"
  },
  "devDependencies": {
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.8",
    "@types/react-native": "0.69.8",
    "@ui-composer/eslint-config": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.6"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-native": "^0.69.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "^0.69.1"
  },
  "peerDependenciesMeta": {
    "@types/react-dom": {
      "optional": true
    },
    "@types/react-native": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "author": "Katherine Martinez <kmartinezmedia@gmail.com>",
  "homepage": "https://github.com/ui-composer/ui-composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-composer/ui-composer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-composer/ui-composer",
    "directory": "packages/ui-composer"
  },
  "types": "./dts/index.d.ts",
  "main": "./lib/native/index.js"
}
