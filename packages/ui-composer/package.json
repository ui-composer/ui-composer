{
  "name": "ui-composer",
  "version": "0.0.0",
  "source": "src",
  "files": [
    "dist",
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "package.json",
    "src",
    "src/**/*.{ts,tsx,json}"
  ],
  "sideEffects": [
    "*.css"
  ],
  "module": "./dist/index.js",
  "react-native": "./dist/index.native.js",
  "types": "./dts/index.d.ts",
  "packemon": [
    {
      "bundle": false,
      "platform": "native"
    }
  ],
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "colord": "^2.9.3",
    "csstype": "^3.1.1",
    "lodash": "^4.17.21",
    "type-fest": "^2.16.0"
  },
  "devDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-native": "*",
    "@ui-composer/eslint-config": "workspace:*",
    "eslint": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "peerDependencies": {
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-native": "0.69.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.1"
  },
  "peerDependenciesMeta": {
    "@types/react-dom": {
      "optional": true
    },
    "@types/react-native": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "author": "Katherine Martinez <kmartinezmedia@gmail.com>",
  "homepage": "https://github.com/ui-composer/ui-composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-composer/ui-composer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-composer/ui-composer",
    "directory": "packages/ui-composer"
  },
  "main": "./lib/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "react-native": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
