{
  "root": "packages/tokens",
  "sourceRoot": "packages/tokens/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/generated"],
      "options": {
        "cwd": "packages/tokens",
        "commands": ["zx ./scripts/build.mjs"]
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/tokens",
        "commands": ["yarn version $npm_package_version --no-git-tag-version"]
      }
    }
  }
}
