{
  "name": "@ui-composer/react",
  "version": "0.0.0",
  "sideEffects": [
    "*.css"
  ],
  "packemon": [
    {
      "bundle": false,
      "platform": "native"
    }
  ],
  "source": "src",
  "types": "./dts/index.d.ts",
  "main": "./lib/index.js",
  "react-native": "./lib/index.native.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "react-native": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./native": {
      "types": "./dts/index.native.d.ts",
      "default": "./lib/index.native.js"
    },
    "./metro-config": "./metro-config.js"
  },
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "@ui-composer/theme": "workspace:^",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@ui-composer/eslint-config": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.70.6"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "^0.70.6"
  },
  "peerDependenciesMeta": {
    "@types/react-dom": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "author": "Katherine Martinez <kmartinezmedia@gmail.com>",
  "homepage": "https://github.com/ui-composer/ui-composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-composer/ui-composer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-composer/ui-composer",
    "directory": "packages/react"
  }
}
